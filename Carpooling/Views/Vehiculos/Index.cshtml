@model IEnumerable<Carpooling.Models.Vehiculo>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
   @* <link rel="stylesheet" href="~/css/StyleTable.css">*@
    <title></title>
</head>
<body>
    <br /><br /><br /><br /><br />
    <p>
        <a asp-action="Create">Agrega un vehiculo!</a>
    </p>
    <div id="container">
        <table>
            <thead>
                <tr>
                    <th>
                        @Html.DisplayNameFor(model => model.Marca)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Modelo)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Color)
                    </th>
                </tr>
            </thead>
            @foreach (var item in Model)
                {
                    <tr>
                        <td>
                        @Html.DisplayFor(modelItem => item.Marca)
                        </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Modelo)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Color)
                    </td>
                    <td>
                        <a asp-action="Edit" asp-route-id="@item.Placa">Edit</a> |
                        <a asp-action="Details" asp-route-id="@item.Placa">Details</a> |
                        <a asp-action="Delete" asp-route-id="@item.Placa">Delete</a>
                    </td>
                </tr>
                }
            @for (int i = 0; i < 2; i++)
                {
                <tr>
                    <td> Juanda</td>
                    <td> Ppapa</td>
                    <td> Leo matsi</td>
                </tr>
            }
        </table>
    </div>
</body>


